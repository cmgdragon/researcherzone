DENO_DIR=$PWD/deno_modules deno cache imports.js

deno run --lock=lock.json --lock-write imports.ts

deno run --allow-env --allow-read --allow-net --config ./tsconfig.json  src/main.ts

deno test --import-map=src/import_map.json --config ./tsconfig.json -A

//docker
docker build -t researcherzone .
docker run -it --init -p 8001:8000 deno_server
